<%template shell(Map params){...}

title = params['title'] ?: 'ESPN API Samples'

%>
<html>
  <head>
    <title><%title%></title>
    <meta content='width=device-width, initial-scale=1.0' name='viewport' />
    <meta content='Samples of client usage of the ESPN API.' name='description' />
    <meta content='Brian R. Jackson' name='author' />
    <!-- HTML5 shim, for IE6-8 support of HTML elements -->
    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js" type="text/javascript"></script>
    <![endif]-->

    <link href="/stylesheets/bootstrap/bootstrap.css" media="screen" rel="stylesheet" type="text/css" />
    <link href="/stylesheets/wedge.css" media="screen" rel="stylesheet" type="text/css" />
    <link href="/stylesheets/bootstrap/responsive.css" media="screen" rel="stylesheet" type="text/css" />
    <link href="/stylesheets/application.css" media="screen" rel="stylesheet" type="text/css" />
 </head>
  <body data-offset='50' data-spy='scroll' data-target='.subnav' onload='javascript:startCarousel();'>
<div class='navbar navbar-fixed-top'>
      <div class='navbar-inner'>
        <div class='container'>
          <a class='btn btn-navbar' data-target='.nav-collapse' data-toggle='collapse'>
            <span class='icon-bar'></span>
            <span class='icon-bar'></span>
            <span class='icon-bar'></span>
          </a>
          <a class='brand' href='/'>ESPN API Samples</a>
          <div class='nav-collapse'>
            <ul class='nav'>
              <li class='active'><a href="/">Welcome</a></li>
              <li><a href="/headlines">Headlines</a></li>
            </ul>
            <ul class='nav pull-right'>
              <li>
                <a href='http://developer.espn.com'>Dev Center</a>
              </li>
              <li>
                <a href='http://espn.github.com'>ESPN on GitHub</a>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
    <div class='container'>
      <header class='jumbotron subhead' id='overview'>
        <h1><%title%></h1>
        <p class='lead'><%params['lead']%></p>
      </header>
<% ... %>
      <hr class='soften' />
      <div id='footer'>
        <img alt="150px wide black short" src="http://a.espncdn.com/i/apis/attribution/espn-api-black_150.png">
      </div>
    </div>
    <script src="/javascripts/jquery.js" type="text/javascript"></script>
    <script src="/javascripts/jquery.webticker.js" type="text/javascript"></script>
    <script src="/javascripts/bootstrap/bootstrap-alert.js" type="text/javascript"></script>
    <script src="/javascripts/bootstrap/bootstrap-button.js" type="text/javascript"></script>
    <script src="/javascripts/bootstrap/bootstrap-carousel.js" type="text/javascript"></script>
    <script src="/javascripts/bootstrap/bootstrap-collapse.js" type="text/javascript"></script>
    <script src="/javascripts/bootstrap/bootstrap-dropdown.js" type="text/javascript"></script>
    <script src="/javascripts/bootstrap/bootstrap-modal.js" type="text/javascript"></script>
    <script src="/javascripts/bootstrap/bootstrap-scrollspy.js" type="text/javascript"></script>
    <script src="/javascripts/bootstrap/bootstrap-tab.js" type="text/javascript"></script>
    <script src="/javascripts/bootstrap/bootstrap-tooltip.js" type="text/javascript"></script>
    <script src="/javascripts/bootstrap/bootstrap-popover.js" type="text/javascript"></script>
    <script src="/javascripts/bootstrap/bootstrap-transition.js" type="text/javascript"></script>
    <script src="/javascripts/bootstrap/bootstrap-typeahead.js" type="text/javascript"></script>
    <script src="/javascripts/application.js" type="text/javascript"></script>
  </body>
</html>